@model IEnumerable<UsuariosViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Listado de Usuarios</h1>
<p>
    <a class="btn btn-outline-success" asp-area="" asp-controller="Usuarios" asp-action="Create">Insertar</a>
</p>

<table class="table table-dark table-striped" id="example1">
    <thead>
        <tr>
            <th>
                Rol
            </th>
            <th>
                Nombre
            </th>
            <th>
                Apellido
            </th>

            <th>
                Email
            </th>
            <th>
                Celular
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.rol_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.usu_Nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.usu_Apellido)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.usu_Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.usu_NumeroCelular)
                </td>


            </tr>
        }
    </tbody>
</table>

