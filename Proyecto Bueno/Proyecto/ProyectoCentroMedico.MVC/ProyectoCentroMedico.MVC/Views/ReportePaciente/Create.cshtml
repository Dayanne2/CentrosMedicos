@model ReporteGeneralViewModel
@{
    ViewData["Title"] = "Create";
}

<h1>Reporte General del Paciente</h1>
<form autocomplete="off" method="post" target="_blank">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="enfer_Inscripcioon" class="control-label col-md-2"></label>
        <div class="col-md-10">
            <select asp-for="enfer_Inscripcioon" class="form-control" asp-items="Model.ListadoPaciente">
                <option>Seleccione el Paciente</option>
            </select>
            <span asp-validation-for="enfer_Inscripcioon" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Mostrar Reporte" class="btn btn-success" target="_blank" />
        </div>
    </div>
</form>

